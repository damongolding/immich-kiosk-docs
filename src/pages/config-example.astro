---
import StarlightPage from "@astrojs/starlight/components/StarlightPage.astro";

import { Code } from "@astrojs/starlight/components";

const config = await fetch(
    "https://raw.githubusercontent.com/damongolding/immich-kiosk/refs/heads/main/config.example.yaml?raw",
);

const myCode = await config.text();
---

<StarlightPage
    frontmatter={{
        title: "config.yaml",
    }}
>
    <Code code={myCode} lang="yaml" title="config.yaml" />
</StarlightPage>
